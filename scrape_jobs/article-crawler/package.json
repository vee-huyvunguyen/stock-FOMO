{
    "name": "article-crawler",
    "version": "0.0.1",
    "type": "module",
    "description": "This is an example of a Crawlee project.",
    "dependencies": {
        "@types/lodash": "^4.17.13",
        "@types/luxon": "^3.4.2",
        "apify": "^3.3.0",
        "axios": "^1.7.9",
        "cheerio": "^1.0.0",
        "crawlee": "^3.0.0",
        "lodash": "^4.17.21",
        "lodash.difference": "^4.5.0",
        "luxon": "^3.5.0",
        "puppeteer": "^23.11.1",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@apify/tsconfig": "^0.1.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.4.0",
        "typescript": "~5.7.0"
    },
    "scripts": {
        "start": "npm run start:dev",
        "start:prod": "node dist/main.js",
        "start:dev": "tsx --require tsconfig-paths/register src/main.ts",
        "quicktest": "tsx --require tsconfig-paths/register src/quicktest.ts",
        "build": "tsc",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:detectOpenHandles": "jest --detectOpenHandles",
        "test:file": "jest --findRelatedTests",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "author": "It's not you it's me",
    "license": "ISC",
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 80,
        "tabWidth": 2
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild",
            "puppeteer"
        ]
    }
}
